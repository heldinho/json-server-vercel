      "total_ingredients": "5",
      "time": 65,
      "cover": "https://i.imgur.com/24tEgVo.jpg",
      "video": "0MJep0plDlw",
      "ingredients": [
        {
          "title": "",
          "list": [
            {
              "id": "1",
              "text": "1 lata de leite condensado"
            },
            {
              "id": "2",
              "text": "1 (mesmo tamanho de leite condensado) de leite"
            },
            {
              "id": "3",
              "text": "3 ovos inteiros"
            },
            {
              "id": "4",
              "text": "1 xícara (chá) açúcar"
            },
            {
              "id": "5",
              "text": "1/2 xícara água"
            }
          ]
        }
      ],
      "instructions": [
        {
          "id": "1",
          "name": "Único",
          "instruction": [
            {
              "id": "1",
              "text": "Primeiro, bata bem os ovos no liquidificador."
            },
            {
              "id": "2",
              "text": "Acrescente o leite condensado e o leite, e bata novamente."
            },
            {
              "id": "3",
              "text": "Derreta o açúcar na panela até ficar moreno, acrescente a água e deixe engrossar."
            },
            {
              "id": "4",
              "text": "Coloque em uma forma redonda e despeje a massa do pudim por cima."
            },
            {
              "id": "5",
              "text": "Asse em forno médio por 45 minutos, com a assadeira redonda dentro de uma maior com água."
            },
            {
              "id": "6",
              "text": "Espete um garfo para ver se está bem assado."
            },
            {
              "id": "7",
              "text": "Deixe esfriar e desenforme."
            }
          ]
        }
      ]
